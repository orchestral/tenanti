{
    "name": "orchestra/tenanti",
    "description": "Tenant based Database Schema Manager for Laravel",
    "keywords": ["laravel", "orchestral", "orchestra-platform", "multi-tenant", "schema", "migration"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Tenanti\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/stubs",
            "tests/Kernel.php"
        ]
    },
    "require": {
        "php": ">=5.6.0",
        "illuminate/filesystem": "~5.3.0",
        "orchestra/support": "~3.3.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "~5.6",
        "laravel/framework": "~5.3.0",
        "orchestra/testbench": "~3.3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.3-dev"
        }
    },
    "minimum-stability": "dev"
}
